    <decisions>
        <name>Is_the_stage_correct</name>
        <label>Is the stage correct?</label>
        <locationX>314</locationX>
        <locationY>623</locationY>
        <defaultConnector>
            <targetReference>Changes_Needing_To_Be_Saved</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Stage_is_incorrect</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>FM_WhatShouldBeTheCurrentStage</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Waiting For Answers From Requestor</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Var_StageChange_for_update</targetReference>
            </connector>
            <label>Stage is incorrect</label>
        </rules>
    </decisions>
    <description>Checks for Changes to trigger if:
Due Date is set and email a notification 
Download link is set and email a notification</description>
    <environments>Default</environments>
    <formulas>
        <name>FM_WhatShouldBeTheCurrentStage</name>
        <dataType>String</dataType>
        <expression>If(
And(
	ISBLANK({!$Record.RFP_Number__c}) = True, ISBLANK(Text({!$Record.RFP_Due_Date__c})) = True, ISBLANK({!$Record.RFP_Download_Link__c}) = True
), &apos;RFP Requested&apos;, 
If(
And(
	ISBLANK({!$Record.RFP_Number__c}) = False, ISBLANK(Text({!$Record.RFP_Due_Date__c})) = False, ISBLANK({!$Record.RFP_Download_Link__c}) = True
), &apos;Due Date &amp; RFP Generated&apos;, 
If(
And(
	ISBLANK({!$Record.RFP_Number__c}) = False, ISBLANK(Text({!$Record.RFP_Due_Date__c})) = False, ISBLANK({!$Record.RFP_Download_Link__c}) = False
), &apos;Estimation Delivered&apos;, &apos;RFP Requested&apos;
)
)

)</expression>
    </formulas>
