    <decisions>
        <name>Has_Stage_Changed_or_Is_new_opp</name>
        <label>Has Stage Changed or Is new opp?</label>
        <locationX>1326</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Stage_has_changed</name>
            <conditionLogic>(1 OR 2) and 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.StageName</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>FM_Proceed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>What_Stage_was_it_set_to</targetReference>
            </connector>
            <label>Yes Stage has changed</label>
        </rules>
    </decisions>
    <decisions>
        <name>What_Stage_was_it_set_to</name>
