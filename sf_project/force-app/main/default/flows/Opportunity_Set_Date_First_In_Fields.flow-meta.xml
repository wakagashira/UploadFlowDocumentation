        <locationX>842</locationX>
        <locationY>431</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Assessing</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Assessing</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity</targetReference>
            </connector>
            <label>Assessing</label>
        </rules>
        <rules>
            <name>Qualified</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Qualified</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Qualified</targetReference>
            </connector>
            <label>Qualified</label>
        </rules>
        <rules>
            <name>Proposal_Generation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Proposal/Audit Generation</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Proposal_Audit</targetReference>
            </connector>
            <label>Proposal Generation</label>
        </rules>
        <rules>
            <name>Proposed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Proposed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Proposed</targetReference>
            </connector>
            <label>Proposed</label>
        </rules>
        <rules>
            <name>Negotiated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Negotiated</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Negotiated</targetReference>
            </connector>
            <label>Negotiated</label>
        </rules>
        <rules>
            <name>Closed_Lost</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed Lost</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Closed_Lost</targetReference>
            </connector>
            <label>Closed Lost</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>FM_Proceed</name>
        <dataType>Boolean</dataType>
        <expression>if(Text({!$Record.StageName}) = &quot;Assessing&quot; &amp;&amp; isnull({!$Record.Date_First_In_Assessing__c}) = True, True, 
if(Text({!$Record.StageName}) = &quot;Qualified&quot; &amp;&amp; isnull({!$Record.Date_First_In_Qualified__c}) = True, True, 
if(Text({!$Record.StageName}) = &quot;Proposal/Audit Generation&quot; &amp;&amp; isnull({!$Record.Date_First_In_Proposal_Audit__c}) = True, True,
if(Text({!$Record.StageName}) = &quot;Proposed&quot; &amp;&amp; isnull({!$Record.Date_First_In_proposed__c}) = True, True, 
if(Text({!$Record.StageName}) = &quot;Negotiated&quot; &amp;&amp; isnull({!$Record.Date_First_In_Negotiating__c}) = True, True, 
if(Text({!$Record.StageName}) = &quot;Closed Lost&quot; &amp;&amp; isnull({!$Record.Date_First_In_Closed_Lost__c}) = True, True,  False))))))</expression>
    </formulas>
