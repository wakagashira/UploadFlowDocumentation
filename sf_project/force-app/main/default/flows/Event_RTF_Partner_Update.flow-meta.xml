        <rules>
            <name>In_the_Past</name>
            <conditionLogic>(1 or 2)  and (2 Or 3) and 4</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StartDateTime</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
