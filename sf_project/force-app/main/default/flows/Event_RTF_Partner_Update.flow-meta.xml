                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Get_Partner_Records.Last_Meeting_Date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.StartDateTime</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>FM_Today</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Last_Meeting</targetReference>
            </connector>
            <label>In the Past</label>
        </rules>
        <rules>
            <name>In_the_future</name>
            <conditionLogic>1 AND (2 or 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.StartDateTime</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>FM_Today</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.StartDateTime</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>Get_Partner_Records.Next_Meeting_Date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Partner_Records.Next_Meeting_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Next_Meeting</targetReference>
            </connector>
            <label>In the future</label>
        </rules>
    </decisions>
